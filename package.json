{
  "name": "@wdk/wallet-evm-erc-4337",
  "version": "1.0.0-beta.1",
  "description": "A simple package to manage BIP-32 wallets for evm blockchains, which implement the erc-4337 standard and its account abstraction features",
  "main": "index.js",
  "type": "module",
  "types": "./types",
  "scripts": {
    "build:types": "rimraf *.d.ts **/*.d.ts; tsc",
    "lint": "standard",
    "lint:fix": "standard --fix"
  },
  "dependencies": {
    "@wdk-safe-global/relay-kit": "4.1.0",
    "@wdk/wallet": "https://github.com/tetherto/wdk-wallet.git#develop",
    "@wdk/wallet-evm": "https://github.com/tetherto/wdk-wallet-evm.git#develop",
    "bare-wdk-runtime": "2.0.0",
    "ethers": "6.14.4"
  },
  "devDependencies": {
    "rimraf": "6.0.1",
    "standard": "17.1.2",
    "typescript": "5.8.3"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "bare": "./bare.js",
      "default": "./index.js"
    },
    "./package": {
      "default": "./package.json"
    }
  }
}
